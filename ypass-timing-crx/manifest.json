{
  "name": "Yahoo Login Password Timing Processing",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Yahoo!ログイン時にタイミングを考慮したパスワードに変更",
  
  "content_scripts": [
    {
      "matches": [ "http://login.yahoo.co.jp/*","https://login.yahoo.co.jp/*" ],
      "js": [ "content.js" ]
    }
  ]
}